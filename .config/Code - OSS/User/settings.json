{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "prettier.printWidth": 70,

  "keyboard.dispatch": "keyCode",
  "liveServer.settings.donotShowInfoMsg": true,
  "terminal.integrated.automationShell.linux": "",
  "terminal.integrated.shell.linux": "fish",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/vscode.vim",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["<C-d>"]
    },
    {
      "before": [":"],
      "after": ["<C-u>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["s"],
      "after": ["S"]
    },
    {
      "before": [":"],
      "after": ["<C-u>"]
    },
    {
      "before": ["L"],
      "after": ["<C-d>"]
    },
    {
      "before": [";"],
      "after": [":"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [",", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": [",", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<TAB>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-TAB>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": [",", "s"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    }
  ],
  //   "workbench.colorCustomizations": {

  //     "editor.background": "#2b2b2b",
  // "editor.lineHighlightBackground" : "#323232",
  // "sideBar.background": "#3c3f41",
  // "tab.activeBackground": "#313335",
  // "tab.inactiveBackground": "#292b2d"
  //   },
  "database-client.preferConnectionName": true,
  "[sql]": {
    "editor.defaultFormatter": "cweijan.vscode-mysql-client2"
  },
  "editor.tabSize": 2,
  "vim.scroll": 30,
  "workbench.colorTheme": "Dartcula Theme - Pure",
  "workbench.iconTheme": "vscode-jetbrains-icon-theme",
  "editor.smoothScrolling": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript"]
}
